      <bind nodeset="/referral/temp_clinical_note" readonly="true()" type="string"/>
      <bind nodeset="/referral/temp_clinical_unit" type="select1"/>
      <bind constraint="(selected(../temp_clinical_unit, 'celsius') and . &gt; 32 and . &lt; 45) or (selected(../temp_clinical_unit, 'fahrenheit') and . &gt; 90 and . &lt; 110)" jr:constraintMsg="jr:itext('/referral/temp_clinical:jr:constraintMsg')" nodeset="/referral/temp_clinical" type="decimal"/>
      <bind nodeset="/referral/referred_to" required="true()" type="select1"/>
      <bind nodeset="/referral/refer_other" relevant="selected(../referred_to, 'other')" required="true()" type="string"/>
      <bind nodeset="/referral/notes" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/referral/meta/instanceID" readonly="true()" type="string"/>
    </model>
